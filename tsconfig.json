{
  "extends": "@tsconfig/node22/tsconfig.json",
  "compilerOptions": {
    "rootDir": "./aoc",
    "baseUrl": "./",
    "outDir": "./dist",
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "newLine": "lf"
  },
  "ts-node": {
    "files": true,
    "esm": true
  },
  "exclude": ["./dist", "./node_modules", "./__tests__", "./coverage"]
}
